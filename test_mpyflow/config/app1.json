{
    "controllers": {
        "http": {
            "call@": "test_mpyflow.mockapp.HttpController",
            "if@": {
                "collection!ref": "active_controllers",
                "contains@": "http"
            },
            "config!ref": "config"
        },
        "bluetooth": {
            "call@": "test_mpyflow.mockapp.BluetoothController",
            "if@": {
                "collection!ref": "active_controllers",
                "contains@": "bluetooth"
            },
            "config!ref": "config"
        },
        "call": {
            "call@": "test_mpyflow.mockapp.InactiveController",
            "if@": {
                "collection!ref": "active_controllers",
                "contains@": "not_a_controller"
            },
            "config!ref": "config"
        }
    },
    "config": {
        "call@": "test_mpyflow.mockapp.load_config",
        "config_class": {
            "call@": "mpyflow.flow.get_callable",
            "path": "test_mpyflow.mockapp.AppConfig"
        }
    },
    "active_controllers": {
        "obj!ref": "config",
        "getattr@": "active_controllers"
    },
    "app": {
        "call@": "test_mpyflow.mockapp.App::create",
        "controllers!ref": "controllers",
        "config!ref": "config"
    }
}